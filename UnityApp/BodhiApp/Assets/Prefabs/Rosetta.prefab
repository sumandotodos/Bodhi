        - theList: []
        - theList: []
        - theList: []
        - theList: []
        - theList: []
        - theList: []
        - theList: []
        - theList: []
        - theList: []
        - theList:
          - key: 16.crsv_0_6
            text: "\xBFQu\xE9 pasar\xEDa si los encargados de tomar decisiones que
              da\xF1an a muchas personas fueran tambi\xE9n los encargados de ejecutarlas
